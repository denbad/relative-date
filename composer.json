{
    "name": "denbad/relative-date",
    "description": "Converts date to a pointer-word analog",
    "type": "library",
    "keywords": ["date", "format", "relative", "pointer-words"],
    "homepage": "https://github.com/denbad/relative-date",
    "license": "MIT",
    "authors": [
        {
          "name": "Denis Badulin"
        }
    ],
    "support": {
        "issues": "https://github.com/denbad/relative-date/issues",
        "source": "https://github.com/denbad/relative-date"
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.4",
        "phpunit/phpunit": "^8.1",
        "phpstan/phpstan": "^0.11.5"
    },
    "suggest": {
        "denbad/relative-date-symfony-bridge": "Bridge for using denbad/relative-date in Symfony projects"
    },
    "autoload": {
        "psr-4": {"Denbad\\RelativeDate\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Denbad\\RelativeDate\\Tests\\": "tests"}
    }
}
